{
    "version": 2,
    "builds": [
      {
        "src": "api/webhook.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxDuration": 10,
          "includeFiles": [
            "Source/python/calculations.py",
            "Source/python/env_config.py"
          ],
          "excludeFiles": [
            "**/*",
            "!api/webhook.py",
            "!Source/python/calculations.py",
            "!Source/python/env_config.py"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/webhook.py"
      }
    ],
    "env": {
      "MINIMAL_ASSETS": "true"
    }
}

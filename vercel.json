{
    "version": 2,
    "builds": [
      {
        "src": "api/webhook.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxDuration": 10,
          "requirementsPath": "vercel-requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/webhook.py"
      }
    ],
    "env": {
      "MINIMAL_ASSETS": "true"
    },
    "functions": {
      "api/**": {

        "excludeFiles": "{*.cache,public,app,__pycache__/,.vercel,.now,.env,output/,temp/}/**"
      }
    }
}
